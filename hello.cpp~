
#include <bits/stdc++.h>
using namespace std;

class A{
int age;
string name;
A(int age,string name){
this->age=age;
this->name=name;
}
void show(string,int);
};

void A::show(string name,int age){
cout << "Hello " << name " Your age is " << age << endl;
}

int main(){
A a("Master",21);
a.show();
}
